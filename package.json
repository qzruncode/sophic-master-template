{
  "name": "sophic-master-template",
  "version": "1.0.0",
  "description": "dark-tunnel的应用模板，用于快速生成sophic应用",
  "scripts": {
    "start": "dark run -m Dev -h local.test.com -H -url /",
    "build": "dark run -m Pro -url /",
    "reinstall": "rm -rf node_modules && npm install --registry=https://registry.npmjs.org",
    "tsc": "tsc",
    "fix": "npx eslint --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "prettier --write",
      "npm run fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qzruncode/sophic-master-template.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.9.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2"
  },
  "dependencies": {
    "antd": "^5.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1"
  }
}
